{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "candidate.jsonl.schema.json",
  "type": "object",
  "additionalProperties": true,
  "required": ["candidate_id", "caller_jar_id", "caller_class_name", "caller_method_name", "caller_method_desc"],
  "properties": {
    "candidate_id": { "type": "string" },
    "caller_jar_id": { "type": "integer" },
    "caller_class_name": { "type": "string" },
    "caller_method_name": { "type": "string" },
    "caller_method_desc": { "type": "string" },
    "callee_class_name": { "type": "string" },
    "callee_method_name": { "type": "string" },
    "callee_method_desc": { "type": "string" },
    "reachable": { "type": ["boolean", "null"] },
    "reach_depth": { "type": ["integer", "null"] },
    "entry_mapping": { "type": ["string", "null"] },
    "priority": { "type": ["integer", "null"] }
  }
}


